{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.lancef":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"lfa":"query.get_equipped_item_name=='lancefa' && query.is_using_item"},
						{"lfb":"query.get_equipped_item_name=='lancefb' && query.is_using_item"},
						{"lfa_a":"query.get_equipped_item_name=='lancefa' && variable.attack_time"},
						{"lfb_a":"query.get_equipped_item_name=='lancefb' && !query.is_using_item && !query.is_sprinting && query.ground_speed - query.vertical_speed > 0.1 && query.ground_speed - query.vertical_speed <= 10"},
						{"lfb_b":"query.get_equipped_item_name=='lancefb' && !variable.attack_time && !query.is_using_item && query.is_sprinting"},
						{"lfb_d":"query.get_equipped_item_name=='lancefb' && variable.attack_time && query.is_sprinting"},
						{"lfb_c":"query.get_equipped_item_name=='lancefb' && !query.is_using_item && !query.is_sprinting && query.ground_speed - query.vertical_speed > 10"}
					]
				},
				"lfa": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 冲击模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancefb"
					]
				},
				"lfb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 普通模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancefa"
					]
				},
				"lfa_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=4,type=!item] 7"
					]
				},
				"lfb_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3,type=!item] 5"
					]
				},
				"lfb_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 7"
					]
				},
				"lfb_c": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 10"
					]
				},
				"lfb_d": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/effect @s speed 1 5 true"
					]
				}
			}
		}
 	}
 }