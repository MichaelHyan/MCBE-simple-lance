{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.lancef":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"lfa":"query.get_equipped_item_name=='lancefa' && query.is_using_item"},
						{"lfb":"query.get_equipped_item_name=='lancefb' && query.is_using_item"},
						{"lfa_a":"query.get_equipped_item_name=='lancefa' && variable.attack_time"},
						{"lfb_a":"query.get_equipped_item_name=='lancefb' && !query.is_sprinting && query.modified_move_speed - query.vertical_speed > 0.1 && query.modified_move_speed - query.vertical_speed <= 5"},
						{"lfb_b":"query.get_equipped_item_name=='lancefb' && query.is_sprinting"},
						{"lfb_c":"query.get_equipped_item_name=='lancefb' && !query.is_sprinting && query.modified_move_speed - query.vertical_speed > 5"}
					]
				},
				"lfa": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 冲击模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancefb"
					]
				},
				"lfb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 普通模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancefa"
					]
				},
				"lfa_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=4] 7"
					]
				},
				"lfb_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3] 5"
					]
				},
				"lfb_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3] 7"
					]
				},
				"lfb_c": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3] 10"
					]
				}
			}
		}
 	}
 }