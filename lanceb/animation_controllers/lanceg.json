{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.lanceg":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"lga":"query.get_equipped_item_name=='lancega' && query.is_using_item"},
						{"lgb":"query.get_equipped_item_name=='lancegb' && query.is_using_item"},
						{"lga_a":"query.get_equipped_item_name=='lancega' && variable.attack_time"},
						{"lgb_a":"query.get_equipped_item_name=='lancegb' && !query.is_using_item && !query.is_sprinting && query.ground_speed - query.vertical_speed > 0.1 && query.ground_speed - query.vertical_speed <= 10"},
						{"lgb_b":"query.get_equipped_item_name=='lancegb' && !variable.attack_time && !query.is_using_item && query.is_sprinting"},
						{"lgb_d":"query.get_equipped_item_name=='lancegb' && variable.attack_time && query.is_sprinting"},
						{"lgb_c":"query.get_equipped_item_name=='lancegb' && !query.is_using_item && !query.is_sprinting && query.ground_speed - query.vertical_speed > 10"}
					]
				},
				"lga": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 冲击模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancegb"
					]
				},
				"lgb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 普通模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancega"
					]
				},
				"lga_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=4,type=!item] 5"
					]
				},
				"lgb_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3,type=!item] 1"
					]
				},
				"lgb_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 3"
					]
				},
				"lgb_c": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 6"
					]
				},
				"lgb_d": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/effect @s speed 1 5 true"
					]
				}
			}
		}
 	}
 }