{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.lanceg":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"lga":"query.get_equipped_item_name=='lancega' && query.is_using_item"},
						{"lgb":"query.get_equipped_item_name=='lancegb' && query.is_using_item"},
						{"lga_a":"query.get_equipped_item_name=='lancega' && variable.attack_time"},
						{"lgb_a":"query.get_equipped_item_name=='lancegb' && !query.is_sprinting && query.modified_move_speed - query.vertical_speed > 0.1 && query.modified_move_speed - query.vertical_speed <= 5"},
						{"lgb_b":"query.get_equipped_item_name=='lancegb' && query.is_sprinting"},
						{"lgb_c":"query.get_equipped_item_name=='lancegb' && !query.is_sprinting && query.modified_move_speed - query.vertical_speed > 5"}
					]
				},
				"lga": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 冲击模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancegb"
					]
				},
				"lgb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 普通模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancega"
					]
				},
				"lga_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=4] 5"
					]
				},
				"lgb_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3] 1"
					]
				},
				"lgb_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3] 3"
					]
				},
				"lgb_c": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3] 6"
					]
				}
			}
		}
 	}
 }