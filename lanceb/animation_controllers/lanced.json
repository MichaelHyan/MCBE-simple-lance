{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.lanced":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"lda":"query.get_equipped_item_name=='lanceda' && query.is_using_item"},
						{"ldb":"query.get_equipped_item_name=='lancedb' && query.is_using_item && !query.is_sprinting"},
						{"lda_a":"query.get_equipped_item_name=='lanceda' && variable.attack_time"},
						{"ldb_a":"query.get_equipped_item_name=='lancedb' && !variable.attack_time && !query.is_using_item && query.ground_speed - query.vertical_speed > 0.1 && query.ground_speed - query.vertical_speed <= 5"},
						{"ldb_b":"query.get_equipped_item_name=='lancedb' && !variable.attack_time && !query.is_using_item && query.ground_speed - query.vertical_speed > 5 && query.ground_speed - query.vertical_speed <= 10"},
						{"ldb_d":"query.get_equipped_item_name=='lancedb' && variable.attack_time && query.is_sprinting"},
						{"ldb_c":"query.get_equipped_item_name=='lancedb' && !variable.attack_time && !query.is_using_item && query.ground_speed - query.vertical_speed > 10 && query.ground_speed - query.vertical_speed <= 30"},
						{"ldb_e":"query.get_equipped_item_name=='lancedb' && !variable.attack_time && !query.is_using_item && query.ground_speed - query.vertical_speed > 30"}
					]
				},
				"lda": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 冲击模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lancedb"
					]
				},
				"ldb": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 普通模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lanceda"
					]
				},
				"lda_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=4,type=!item] 6"
					]
				},
				"ldb_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3,type=!item] 3"
					]
				},
				"ldb_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 5"
					]
				},
				"ldb_c": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 8"
					]
				},
				"ldb_d": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/effect @s speed 1 5 true"
					]
				},
				"ldb_e": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 16"
					]
				}
			}
		}
 	}
 }