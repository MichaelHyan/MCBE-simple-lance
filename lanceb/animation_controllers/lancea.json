{
	"format_version": "1.8.0",
	"animation_controllers": {
		"controller.animation.lancea":{
			"states":{
				"base_jeta":{
					"transitions":[
						{"laa":"query.get_equipped_item_name=='lanceaa' && query.is_using_item"},
						{"lab":"query.get_equipped_item_name=='lanceab' && query.is_using_item"},
						{"laa_a":"query.get_equipped_item_name=='lanceaa' && variable.attack_time"},
						{"lab_a":"query.get_equipped_item_name=='lanceab' && !variable.attack_time && !query.is_using_item && query.ground_speed - query.vertical_speed > 0.1 && query.ground_speed - query.vertical_speed <= 5"},
						{"lab_b":"query.get_equipped_item_name=='lanceab' && !variable.attack_time && !query.is_using_item && query.ground_speed - query.vertical_speed > 5 && query.ground_speed - query.vertical_speed <= 10"},
						{"lab_d":"query.get_equipped_item_name=='lanceab' && variable.attack_time && query.is_sprinting"},
						{"lab_c":"query.get_equipped_item_name=='lanceab' && !variable.attack_time && !query.is_using_item && query.ground_speed - query.vertical_speed > 10 && query.ground_speed - query.vertical_speed <= 30"},
						{"lab_e":"query.get_equipped_item_name=='lanceab' && !variable.attack_time && !query.is_using_item && query.ground_speed - query.vertical_speed > 30"}
					]
				},
				"laa": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 冲击模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lanceab"
					]
				},
				"lab": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/title @s actionbar 普通模式",
						"/replaceitem entity @s slot.weapon.mainhand 0 items:lanceaa"
					]
				},
				"laa_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=4,type=!item] 4"
					]
				},
				"lab_a": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/damage @e[rm=1,r=3,type=!item] 1"
					]
				},
				"lab_b": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 3"
					]
				},
				"lab_c": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 6"
					]
				},
				"lab_d": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/particle minecraft:balloon_gas_particle ~ ~ ~",
						"/effect @s speed 1 5 true"
					]
				},
				"lab_e": {
					"transitions": [
							{
								"base_jeta": "(1.0)"
							}
					],
					"on_entry": [
						"/particle minecraft:basic_flame_particle ~ ~ ~",
						"/damage @e[rm=1,r=3,type=!item] 12"
					]
				}
			}
		}
 	}
 }